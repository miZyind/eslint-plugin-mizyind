{
  "name": "eslint-plugin-mizyind",
  "version": "2.0.1",
  "description": "This package provides miZyind's custom lint.",
  "repository": "git@github.com:miZyind/eslint-plugin-mizyind.git",
  "author": "miZyind <mizyind@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/miZyind/eslint-plugin-mizyind#readme",
  "bugs": {
    "url": "https://github.com/miZyind/eslint-plugin-mizyind/issues"
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "rimraf lib && tsc --build tsconfig.base.json",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "test": "jest"
  },
  "main": "lib",
  "files": [
    "docs",
    "lib"
  ],
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/experimental-utils": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "lodash.kebabcase": "^4.1.1",
    "prettier": "^2.1.2",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/node": "^14.11.8",
    "eslint-plugin-mizyind": "file:.",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:mizyind/base"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}
