{
  "name": "eslint-plugin-mizyind",
  "version": "2.3.0",
  "description": "This package provides miZyind's custom lint.",
  "author": "miZyind <mizyind@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:miZyind/eslint-plugin-mizyind.git",
  "homepage": "https://github.com/miZyind/eslint-plugin-mizyind#readme",
  "bugs": "https://github.com/miZyind/eslint-plugin-mizyind/issues",
  "main": "lib",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "rimraf lib && tsc --build tsconfig.base.json",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "test": "jest"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/experimental-utils": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "lodash.kebabcase": "^4.1.1",
    "prettier": "^2.2.1",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^26.0.22",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/node": "^14.14.37",
    "eslint-plugin-mizyind": "2.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  },
  "eslintConfig": {
    "extends": [
      "plugin:mizyind/base"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "files": [
    "docs",
    "lib"
  ],
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --max-warnings 0"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
