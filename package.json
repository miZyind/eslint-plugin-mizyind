{
  "name": "eslint-plugin-mizyind",
  "version": "0.0.0-development",
  "description": "This package provides miZyind's custom lint",
  "repository": "git@github.com:miZyind/eslint-plugin-mizyind.git",
  "author": "miZyind <mizyind.dev>",
  "license": "MIT",
  "homepage": "https://github.com/miZyind/eslint-plugin-mizyind#readme",
  "bugs": "https://github.com/miZyind/eslint-plugin-mizyind/issues",
  "main": "lib",
  "scripts": {
    "test": "jest",
    "lint": "eslint --max-warnings 0",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@1.22.22",
  "dependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@next/eslint-plugin-next": "^15.0.3",
    "@stylistic/eslint-plugin": "^2.11.0",
    "@stylistic/eslint-plugin-jsx": "^2.11.0",
    "@stylistic/eslint-plugin-ts": "^2.11.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import-x": "^4.4.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-native": "^4.1.0",
    "globals": "^15.12.0",
    "lodash.kebabcase": "^4.1.1",
    "minimatch": "^10.0.1",
    "prettier": "^3.4.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "semantic-release": "^24.2.0"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "."
        }
      ],
      "@semantic-release/github"
    ]
  }
}
