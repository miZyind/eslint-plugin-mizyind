{
  "name": "eslint-plugin-mizyind",
  "version": "0.0.0-development",
  "description": "This package provides miZyind's custom lint",
  "repository": "git@github.com:miZyind/eslint-plugin-mizyind.git",
  "author": "miZyind <mizyind.dev>",
  "license": "MIT",
  "homepage": "https://github.com/miZyind/eslint-plugin-mizyind#readme",
  "bugs": "https://github.com/miZyind/eslint-plugin-mizyind/issues",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint --max-warnings 0",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": ">=22",
    "yarn": "1.22.22"
  },
  "packageManager": "yarn@1.22.22",
  "dependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@next/eslint-plugin-next": "^15.2.3",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@stylistic/eslint-plugin-jsx": "^4.2.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-import-x": "^4.9.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "globals": "^16.0.0",
    "lodash.kebabcase": "^4.1.1",
    "minimatch": "^10.0.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "semantic-release": "^24.2.3"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "."
        }
      ],
      "@semantic-release/github"
    ]
  }
}
