{
  "name": "eslint-plugin-mizyind",
  "version": "0.0.0-development",
  "description": "This package provides miZyind's custom lint.",
  "repository": "git@github.com:miZyind/eslint-plugin-mizyind.git",
  "author": "miZyind <mizyind.dev>",
  "license": "MIT",
  "homepage": "https://github.com/miZyind/eslint-plugin-mizyind#readme",
  "bugs": "https://github.com/miZyind/eslint-plugin-mizyind/issues",
  "main": "lib",
  "scripts": {
    "lint": "eslint --max-warnings 0",
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@next/eslint-plugin-next": "^14.2.5",
    "@stylistic/eslint-plugin": "^2.4.0",
    "@stylistic/eslint-plugin-jsx": "^2.4.0",
    "@stylistic/eslint-plugin-ts": "^2.4.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import-x": "^3.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "globals": "^15.8.0",
    "lodash.kebabcase": "^4.1.1",
    "minimatch": "^10.0.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "semantic-release": "^24.0.0"
  },
  "files": [
    "docs",
    "lib"
  ],
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "."
        }
      ],
      "@semantic-release/github"
    ]
  }
}
