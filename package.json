{
  "name": "eslint-plugin-mizyind",
  "version": "0.0.0-development",
  "description": "This package provides miZyind's custom lint.",
  "repository": "git@github.com:miZyind/eslint-plugin-mizyind.git",
  "author": "miZyind <mizyind@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/miZyind/eslint-plugin-mizyind#readme",
  "bugs": "https://github.com/miZyind/eslint-plugin-mizyind/issues",
  "main": "lib",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "rimraf lib && tsc --build tsconfig.base.json",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^13.1.6",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/experimental-utils": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "lodash.kebabcase": "^4.1.1",
    "minimatch": "^6.1.6",
    "prettier": "^2.8.4",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/jest": "^29.4.0",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^18.13.0",
    "eslint-plugin-mizyind": "file:.",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "lint-staged": "^13.1.1",
    "rimraf": "^4.1.2",
    "semantic-release": "^20.1.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "files": [
    "docs",
    "lib"
  ],
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:mizyind/base"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --max-warnings 0"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failComment": false
        }
      ]
    ]
  }
}
